@model Full_Identity.DTOs.CreateSchoolDTO
<h1>Create school</h1>
<form class="row g-3" style="width: 400px" asp-controller="School" asp-action="CreateSchool" enctype="multipart/form-data" method="post">
    <div>
        <input  name="Name" class="form-control" asp-for="Name" type="text" placeholder="Name"/>
        <span asp-validation-for="Name"></span>
    </div>
    <div>
        <input  name="Description" class="form-control" asp-for="Description" type="text" placeholder="Description"/>
        <span asp-validation-for="Description"></span>
    </div>  
     <div>
           <input name="Photo" class="form-control" asp-for="Photo" type="file" />
           <span asp-validation-for="Photo"></span>
       </div>
    <button class="form-control"  type="submit" >Add</button>
</form>
